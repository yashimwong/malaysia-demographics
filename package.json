{
  "name": "malaysia-demographics",
  "version": "0.0.6",
  "description": "A utility library for Malaysian demographics.",
  "homepage": "https://github.com/yashimwong/malaysia-demographics",
  "repository": "github:yashimwong/malaysia-demographics",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepare": "npm run build"
  },
  "keywords": [
    "malaysia",
    "ethnic",
    "demographics",
    "race",
    "bumiputera"
  ],
  "author": {
    "name": "Yashim Wong",
    "email": "yashimwong@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.5.0",
    "typescript": "^5.0.4"
  }
}
